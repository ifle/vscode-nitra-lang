{
    "name": "nitra",
    "displayName": "Nitra Syntax Highlighting",
    "description": "Nitra Syntax Highlighting",
    "version": "0.0.5",
    "publisher": "igorfle",
    "repository": {
        "type": "git",
        "url": "https://github.com/ifle/vscode-nitra-lang.git"
    },
    "bugs": {
        "url": "https://github.com/ifle/vscode-nitra-lang/issues"
    },
    "icon": "nitra-logo.png",
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "nitra",
            "aliases": ["Nitra", "nitra"],
            "extensions": [".nitra"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "nitra",
            "scopeName": "source.nitra",
            "path": "./syntaxes/nitra.tmLanguage.json"
        }]
    }
}